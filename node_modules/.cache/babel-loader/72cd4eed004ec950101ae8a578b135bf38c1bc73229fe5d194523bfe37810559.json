{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport xmlbuilder from 'xmlbuilder';\nexport default {\n  data: () => ({\n    age_min: 0,\n    age_max: 120,\n    core_temp_min: 0,\n    core_temp_max: 70,\n    heart_rate_min: 0,\n    heart_rate_max: 350,\n    resp_rate_min: 0,\n    resp_rate_max: 200,\n    height_min: 0,\n    height_max: 200,\n    weight_min: 0,\n    weight_max: 200,\n    med_options: ['A', 'B', 'C'],\n    tab: 'home',\n    name: [],\n    drawer: false,\n    valid: true,\n    expanded: [0],\n    capability: [{\n      required: [],\n      name: [],\n      data: [],\n      propertyName: [],\n      dataType: [],\n      value: []\n    }],\n    patient_props: {\n      \"Name\": [],\n      \"title\": [],\n      \"age\": 50,\n      \"Sex\": [],\n      \"height\": 150,\n      \"weight\": 75\n    },\n    patient_vitals: {\n      \"DiastolicArterialPressureBaseline\": [],\n      \"central_venous_pressure\": [],\n      \"SystolicArterialPressureBaseline\": [],\n      \"mean_arterial_pressure\": [],\n      \"heart_rate\": 60,\n      \"resp_rate\": 14,\n      \"cardiac_output\": [],\n      \"core_temperature\": 37.5\n    },\n    genders: ['Male', 'Female'],\n    blood_types: ['A+', 'B+', 'AB+', 'A-', 'B-', 'AB-', 'O+', 'O-']\n  }),\n  methods: {\n    nextTab(id) {\n      this.tab = id;\n    },\n    removeCapability(index) {\n      this.capability.splice(index, 1);\n    },\n    validate() {\n      this.$refs.form.validate();\n    },\n    reset() {\n      this.$refs.form.reset();\n    },\n    pathClicked(id) {\n      let pathElement = document.getElementById(id);\n      if (this.name.some(x => Object.keys(x).includes(id))) {\n        this.name = this.name.filter(x => !Object.keys(x).includes(id));\n      } else {\n        this.name.push({\n          [id]: ''\n        });\n      }\n      if (pathElement.classList.contains(\"active\")) {\n        pathElement.classList.remove(\"active\");\n      } else {\n        pathElement.classList.add(\"active\");\n      }\n    },\n    saveXML() {\n      const xml = xmlbuilder.create('Patient', {\n        encoding: 'UTF-8',\n        standalone: \"yes\"\n      });\n      xml.att({\n        'xmlns': \"uri:/mil/tatrc/physiology/datamodel\",\n        'xmlns:xsi': 'http://www.w3.org/2001/XMLSchema-instance',\n        'contentVersion': \"BioGears_7.5\"\n      });\n      //const Patient = xml.ele('Patient')\n      // Patient.att({'name':this.patient_props['name']})\n      for (var key1 in this.patient_props) {\n        if (key1 == 'age') {\n          const Age = xml.ele('Age');\n          Age.att('value', this.patient_props[key1]);\n          Age.att('units', 'yr');\n        } else if (key1 == 'height') {\n          const Height = xml.ele('Height');\n          Height.att('value', this.patient_props[key1]);\n          Height.att('units', '');\n        } else if (key1 == 'weight') {\n          const Weight = xml.ele('Weight');\n          Weight.att('value', this.patient_props[key1]);\n          Weight.att('units', 'kg');\n        } else {\n          xml.ele(key1, this.patient_props[key1]);\n        }\n      }\n      for (var key2 in this.patient_vitals) {\n        if (Array.isArray(this.patient_vitals[key2])) {\n          for (var item2 in this.patient_vitals[key2]) {\n            xml.ele(key2, this.patient_vitals[key2][item2]);\n          }\n        } else {\n          const element = xml.ele(key2, this.patient_vitals[key2]);\n          if (key2 == 'SystolicArterialPressureBaseline') {\n            element.att('units', 'mmHg');\n          }\n          if (key2 == 'DiastolicArterialPressureBaseline') {\n            element.att('units', 'mmHg');\n          }\n          if (key2 == 'pressure') {\n            element.att('units', 'mmHg');\n          }\n        }\n      }\n      var xmlString = xml.end({\n        pretty: true\n      });\n      const blob = new Blob([xmlString], {\n        type: 'text/xml'\n      });\n      const link = document.createElement('a');\n      link.href = URL.createObjectURL(blob);\n      link.download = 'data.xml';\n      link.click();\n    }\n  }\n};","map":{"version":3,"mappings":";AAqXA,OAAOA,UAAS,MAAO,YAAW;AAChC,eAAe;EACbC,IAAI,EAAE,OAAO;IACXC,OAAO,EAAE,CAAC;IACVC,OAAO,EAAE,GAAG;IACZC,aAAa,EAAE,CAAC;IAChBC,aAAa,EAAE,EAAE;IACjBC,cAAc,EAAE,CAAC;IACjBC,cAAc,EAAE,GAAG;IACnBC,aAAa,EAAE,CAAC;IAChBC,aAAa,EAAE,GAAG;IAClBC,UAAU,EAAE,CAAC;IACbC,UAAU,EAAE,GAAG;IACfC,UAAU,EAAE,CAAC;IACbC,UAAU,EAAE,GAAG;IACfC,WAAW,EAAE,CAAC,GAAG,EAAE,GAAG,EAAE,GAAG,CAAC;IAC5BC,GAAG,EAAE,MAAM;IACXC,IAAI,EAAE,EAAE;IACRC,MAAM,EAAE,KAAK;IACbC,KAAK,EAAE,IAAI;IACXC,QAAQ,EAAE,CAAC,CAAC,CAAC;IACbC,UAAU,EAAE,CACV;MAAEC,QAAQ,EAAE,EAAE;MACZL,IAAI,EAAE,EAAE;MACRf,IAAI,EAAE,EAAE;MACRqB,YAAY,EAAE,EAAE;MAChBC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE;IAAG,CAAC,CACd;IAEDC,aAAa,EAAE;MACb,MAAM,EAAE,EAAE;MACV,OAAO,EAAE,EAAE;MACX,KAAK,EAAE,EAAE;MACT,KAAK,EAAE,EAAE;MACT,QAAQ,EAAE,GAAG;MACb,QAAQ,EAAE;IACZ,CAAC;IACDC,cAAc,EAAE;MACd,mCAAmC,EAAE,EAAE;MACvC,yBAAyB,EAAE,EAAE;MAC7B,kCAAkC,EAAC,EAAE;MACrC,wBAAwB,EAAE,EAAE;MAC5B,YAAY,EAAE,EAAE;MAChB,WAAW,EAAE,EAAE;MACf,gBAAgB,EAAE,EAAE;MACpB,kBAAkB,EAAE;IACtB,CAAC;IACDC,OAAO,EAAE,CACP,MAAM,EACN,QAAO,CACR;IACDC,WAAW,EAAE,CACX,IAAI,EACJ,IAAI,EACJ,KAAK,EACL,IAAI,EACJ,IAAI,EACJ,KAAK,EACL,IAAI,EACJ,IAAG;EAEP,CAAC,CAAC;EACFC,OAAO,EAAE;IACLC,OAAO,CAACC,EAAE,EAAE;MACV,IAAI,CAAChB,GAAE,GAAIgB,EAAE;IACf,CAAC;IACDC,gBAAgB,CAACC,KAAK,EAAE;MACtB,IAAI,CAACb,UAAU,CAACc,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAClC,CAAC;IACDE,QAAQ,GAAG;MACT,IAAI,CAACC,KAAK,CAACC,IAAI,CAACF,QAAQ,EAAC;IAC3B,CAAC;IACDG,KAAK,GAAG;MACN,IAAI,CAACF,KAAK,CAACC,IAAI,CAACC,KAAK,EAAC;IACxB,CAAC;IACDC,WAAW,CAACR,EAAE,EAAE;MACd,IAAIS,WAAU,GAAIC,QAAQ,CAACC,cAAc,CAACX,EAAE,CAAC;MAC7C,IAAI,IAAI,CAACf,IAAI,CAAC2B,IAAI,CAACC,KAAKC,MAAM,CAACC,IAAI,CAACF,CAAC,CAAC,CAACG,QAAQ,CAAChB,EAAE,CAAC,CAAC,EAAE;QACpD,IAAI,CAACf,IAAG,GAAI,IAAI,CAACA,IAAI,CAACgC,MAAM,CAACJ,KAAK,CAACC,MAAM,CAACC,IAAI,CAACF,CAAC,CAAC,CAACG,QAAQ,CAAChB,EAAE,CAAC,CAAC;MACjE,OAAO;QACL,IAAI,CAACf,IAAI,CAACiC,IAAI,CAAC;UACb,CAAClB