rd class=\"mt-n12\">\n          <v-card-text>\n            <p style=\"font-size: 20px\">Please upload your .CSV:</p>\n            <br>\n            <v-file-input v-model=\"file\" accept=\".csv\" @change=\"parseCsv\" ref=\"fileInput\"></v-file-input>\n          </v-card-text>\n        </v-card>\n      </v-container>\n    </v-main>\n  </template>\n  \n  <script>\n  import * as d3 from 'd3';\n  \n  export default {\n    name: 'CsvParser',\n    data() {\n      return {\n        headers: [],\n        items: [],\n      };\n    },\n    methods: {\n      async parseCsv() {\n        console.log('Parsing CSV...');\n        try {\n          const file = this.$refs.fileInput.files[0];\n          console.log('File:', file);\n          const text = await file.text();\n          console.log('Text:', text);\n          const csv = d3.csvParse(text);\n          console.log('CSV:', csv);\n          this.headers = Object.keys(csv[0]);\n          this.items = csv;\n        } catch (error) {\n          console.error(error);\n        }\n      },\n    },\n  };\n  </script>\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}